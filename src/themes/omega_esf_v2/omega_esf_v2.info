name = Omega ESF v2
description = Second version of theme for the ESF project.
base theme = omega
screenshot = screenshot.png
engine = phptemplate
core = 7.x

; ========================================
; Stylesheets
; ========================================
stylesheets[all][] = css/omega-esf-v2.normalize.css
stylesheets[all][] = css/omega-esf-v2.hacks.css
stylesheets[all][] = css/omega-esf-v2.styles.css
stylesheets[all][] = css/omega-esf-v2.no-query.css

; ========================================
; Scripts
; ========================================
scripts[] = js/omega-esf-v2.behaviors.js

; ========================================
; Regions
; ========================================
regions[header] = Header
regions[content] = Content
regions[sidebar] = Sidebar
regions[postscript] = Postscript
regions[footer] = Footer

fonts[google_fonts_api][] = "BenchNine&subset=latin-ext#300"